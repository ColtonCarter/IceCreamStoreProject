<AnchorPane prefHeight="605.0" prefWidth="723.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="icecreamshop.DisplayCustomers">
   <children>
      <Button fx:id="order" layoutX="447.0" layoutY="500.0" mnemonicParsing="false" prefHeight="57.0" prefWidth="199.0" text="---&gt; Order">

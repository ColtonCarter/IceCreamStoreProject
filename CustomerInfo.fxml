          <TableColumn fx:id="col_fname" prefWidth="174.0" text="First Name" />
          <TableColumn fx:id="col_lname" minWidth="0.0" prefWidth="191.0" text="Last Name" />
            <TableColumn fx:id="col_flavor" minWidth="0.0" prefWidth="201.0" text="Fave Flavor" />
            <TableColumn fx:id="col_id" minWidth="0.0" prefWidth="94.0" text="ID" />
        </columns>
      </TableView>
      <Label fx:id="title" layoutX="228.0" layoutY="20.0" text="Customers" textFill="#0286cc">
